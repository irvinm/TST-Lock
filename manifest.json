{
  "browser_action": {
    "default_icon": "lock.png"
  },
  "description": "Lock important tabs from being accidentally closed.",
  "homepage_url": "https://github.com/irvinm/TST-Lock",
  "manifest_version": 2,
  "name": "TST-Lock",
  "version": "1.0.2",
  "permissions": ["activeTab","background","find","notifications","sessions","storage","tabs","theme","unlimitedStorage"],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "128": "lock.png"
  },
	"applications": {
		"gecko": {
			"id": "TST-Lock@irvinm.addons.mozilla.org"
		}
	}
}
